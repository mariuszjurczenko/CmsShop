@model IEnumerable<CmsShop.Models.ViewModels.Pages.PageVM>

@{
    string tempPageTitle = (string)TempData["PageTitle"];
}

@foreach (var item in Model)
{
    if (tempPageTitle == item.Title)
    {
        <li class="active"><a href="/@item.Slug"><i class="fa fa-dashboard"></i>@item.Title</a></li>
    }
    else
    {
        <li><a href="/@item.Slug"><i class="fa fa-dashboard"></i>@item.Title</a></li>
    }
}